    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1"
  },
  "eslintConfig": {
    "extends": "airbnb"
  },
  "ava": {
    "files": [
      "test/tests.js"
    ],
    "require": [
      "babel-register"
    ]
