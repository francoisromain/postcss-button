{
  "name": "postcss-button",
  "version": "0.3.10",
  "description": "A PostCSS plugin to create buttons.",
  "keywords": ["postcss", "css", "postcss-plugin", "buttons", "system.css"],
  "author": "Francois Romain <francoisromain@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/francoisromain/postcss-button.git"
  },
  "bugs": {
    "url": "https://github.com/francoisromain/postcss-button/issues"
  },
  "homepage": "http://francoisromain.github.io/postcss-button/",
  "dependencies": {
    "postcss": "^6.0.19",
    "ava": "^0.25.0"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "npm run build && ava && eslint *.js"
  },
  "main": "./lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ]
    }
  },
  "ava": {
    "files": ["test/tests.js"],
    "require": ["babel-register"]
  },
  "babel": {
    "presets": ["es2015"]
  }
}
